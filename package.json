{
  "name": "cpapp",
  "version": "1.0.0",
  "description": "A simple CAP project.",
  "repository": "<Add your repository here>",
  "license": "UNLICENSED",
  "private": true,
  "dependencies": {
    "@sap/cds": "^4",
    "@sap/xsenv": "^3.0.0",
    "@sap/xssec": "^3.0.10",
    "express": "^4",
    "hdb": "^0.18.1",
    "passport": "^0.4.1",
    "@sap/xb-msg-amqp-v100": "^0.9.39"
  },
  "devDependencies": {
    "@sap/ux-specification": "^1.78.10",
    "sqlite3": "^5"
  },
  "scripts": {
    "start": "npx cds run"
  },
  "sapux": [
    "app/risks",
    "app/notification"
  ],
  "cds": {
    "requires": {
      "db": {
        "kind": "sql"
      },
      "uaa": {
        "kind": "xsuaa",
        "credentials": {}
      },
      "[production]": {
        "messaging": {
          "kind": "enterprise-messaging"
        }
      },
      "API_BUSINESS_PARTNER": {
        "kind": "odata",
        "model": "srv/external/API_BUSINESS_PARTNER",
        "credentials": {
          "destination": "cap-api098"
        }
      }
    },
    "hana": {
      "deploy-format": "hdbtable"
    }
  }
}
